  <files>
    <file src="NuGetUtils.MSBuild.Exec\build\*.props" target="build\" />
    <file src="NuGetUtils.MSBuild.Exec\buildMultiTargeting\*.props" target="buildMultiTargeting\" />
    
    <file src="NuGetUtils.MSBuild.Exec\net46\*.dll" exclude="NuGetUtils.MSBuild.Exec\net46\Microsoft.Build.Framework.dll" target="build\net46" />
    <file src="NuGetUtils.MSBuild.Exec\build\net46\tools\*.dll" target="build\net46\tools" />

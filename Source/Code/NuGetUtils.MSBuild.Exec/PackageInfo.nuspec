    <file src="NuGetUtils.MSBuild.Exec\netcoreapp2.1\*.dll" exclude="NuGetUtils.MSBuild.Exec\netcoreapp2.1\Microsoft.Build.Framework.dll;NuGetUtils.MSBuild.Exec\netcoreapp2.1\Newtonsoft.Json.dll" target="build\netcoreapp2.1" />
    <file src="NuGetUtils.MSBuild.Exec\build\netcoreapp2.1\tools\*.dll" target="build\netcoreapp2.1\tools" />
    <file src="NuGetUtils.MSBuild.Exec\build\netcoreapp2.1\tools\*.exe*" target="build\netcoreapp2.1\tools" />

